<html>
    <head>
        <title>Handset details</title>
    </head>
    <body>
    	<img src="/lib/images/erlang.jpg"/>
    	<h1>Device</h1>
		<ul>
			<li>Id            : {{id}}</li>  
			<li>Brand name    : {{brand_name}}</li>
			<li>Model name    : {{model_name}}</li>
			<li>User Agent    : {{user_agent}}</li>
			<li>Actual device : {{actual_device}}</li>
			<li>Fallback      : {{fall_back}}</li>
			<li>created       : {{created|date:"r"}}</li>
			<li>last modified : {{lastmodified|date:"r"}}</li>
		</ul>
		<h1>Groups</h1>
		<ul>
		{% for id, caps in groups %}
		<li>{{ id }}</li>
		{% endfor %}
		</ul>
    </body>
</html>
